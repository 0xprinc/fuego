package templa

templ htmlPage(title string) {
	<!DOCTYPE html>
	<html lang="en">
		<head>
			@Head(title)
		</head>
		<body class="m-0">
			<div class="flex flex-col md:flex-row">
				@AdminNavbar()
				<main id="page" class="container mx-auto p-4">
					{ children... }
				</main>
			</div>
			@Footer()
			@Scripts()
		</body>
	</html>
}
